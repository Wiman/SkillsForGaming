<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.andreas.game.GResActivity">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">


        <TextView
            android:id="@+id/textView14"
            android:layout_width="163dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView5"
            android:layout_marginTop="49dp"
            android:text="  Dåligt    Mellan       Bra"
            app:layout_constraintBottom_toTopOf="@+id/seekbar1"
            app:layout_constraintEnd_toEndOf="@+id/seekbar1"
            app:layout_constraintHorizontal_bias="0.758"
            app:layout_constraintStart_toStartOf="@+id/seekbar1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="123dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:maxWidth="@dimen/_30sdp"
            android:text="@string/resultat"
            android:textColor="@android:color/background_dark"
            android:textSize="30sp"
            tools:layout_editor_absoluteX="48dp"
            tools:layout_editor_absoluteY="79dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="190dp"
            android:layout_height="330dp"

            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/seekBar2"
            android:text="@string/hej"
            android:textColor="@android:color/background_dark"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="200dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="13dp"
            android:text="     Här kommer ett sjukt bra jobbtips!"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp" />

        <SeekBar
            android:id="@+id/seekBar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignStart="@+id/textView14"
            android:layout_below="@+id/textView14" />

        <SeekBar
            android:id="@+id/seekBar3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar2"
            android:layout_alignStart="@+id/seekBar2"
            android:layout_below="@+id/seekBar2"
            android:layout_marginTop="15dp" />

        <SeekBar
            android:id="@+id/seekBar4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar3"
            android:layout_alignStart="@+id/seekBar3"
            android:layout_below="@+id/seekBar3"
            android:layout_marginTop="15dp" />

        <SeekBar
            android:id="@+id/seekBar5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar4"
            android:layout_alignStart="@+id/seekBar4"
            android:layout_below="@+id/seekBar4"
            android:layout_marginTop="14dp" />

        <SeekBar
            android:id="@+id/seekBar6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar5"
            android:layout_alignStart="@+id/seekBar5"
            android:layout_below="@+id/seekBar5"
            android:layout_marginTop="14dp" />

        <SeekBar
            android:id="@+id/seekBar7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar6"
            android:layout_alignStart="@+id/seekBar6"
            android:layout_below="@+id/seekBar6"

            android:layout_marginTop="15dp" />

        <SeekBar
            android:id="@+id/seekBar8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar7"
            android:layout_alignStart="@+id/seekBar7"
            android:layout_below="@+id/seekBar7"
            android:layout_marginTop="15dp" />

        <SeekBar
            android:id="@+id/seekBar9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar8"
            android:layout_alignStart="@+id/seekBar8"
            android:layout_below="@+id/seekBar8"
            android:layout_marginTop="15dp" />

        <SeekBar
            android:id="@+id/seekBar10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/seekBar9"
            android:layout_alignStart="@+id/seekBar9"
            android:layout_below="@+id/seekBar9"
            android:layout_marginTop="15dp" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
