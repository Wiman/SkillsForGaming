<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.andreas.game.SkillsActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <CheckBox
        android:id="@+id/Konfliktlösningsförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Konfliktlösningsförmåga"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="66dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/Kommunikationsförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kommunikationsförmåga"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/Konfliktlösningsförmåga"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/Konfliktlösningsförmåga" />

    <CheckBox
        android:id="@+id/Sammarbetsförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sammarbetsförmåga"
        android:layout_marginStart="9dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Konfliktlösningsförmåga"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/Kommunikationsförmåga"
        android:layout_marginLeft="9dp" />

    <CheckBox
        android:id="@+id/Flexibel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Flexibel"
        android:layout_marginStart="16dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Problemlösning"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="12dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/Tålamod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tålamod"
        android:layout_marginStart="9dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/Kommunikationsförmåga"
        android:layout_marginLeft="9dp" />

    <CheckBox
        android:id="@+id/Problemlösning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Problemlösning"
        android:layout_marginStart="16dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="6dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/Fokus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fokus"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/Prioriteringsförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prioriteringsförmåga"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toBottomOf="@+id/Fokus"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/Besluttagningförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Besluttagningförmåga"
        android:layout_marginEnd="24dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginRight="24dp" />

    <CheckBox
        android:id="@+id/Initiativtagare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initiativtagare"
        android:layout_marginStart="16dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="81dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Relationskompetens"
        android:textSize="20sp"
        app:layout_constraintRight_toLeftOf="@+id/Sammarbetsförmåga"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Konfliktlösningsförmåga"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/Konfliktlösningsförmåga"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="224dp"
        android:layout_height="43dp"
        android:text="Anpassningskompetens"
        android:textSize="20sp"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/Flexibel"
        android:layout_marginTop="44dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="42dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Kommunikationsförmåga"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="220dp"
        android:layout_height="38dp"
        android:text="Prioriteringskompetens"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/Problemlösning"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="231dp"
        android:layout_height="33dp"
        android:text="Organisationskompetens"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Ledarskapsförmåga"
        app:layout_constraintRight_toRightOf="@+id/Initiativtagare"
        android:layout_marginTop="57dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="59dp"
        app:layout_constraintTop_toTopOf="@+id/Prioriteringsförmåga"
        app:layout_constraintLeft_toRightOf="@+id/Initiativtagare" />

    <CheckBox
        android:id="@+id/Ansvarstagande"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ansvarstagande"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="39dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/Besluttagningförmåga"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/Prioriteringsförmåga"
        android:layout_marginLeft="39dp"
        android:layout_marginRight="39dp" />

    <CheckBox
        android:id="@+id/Ledarskapsförmåga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ledarskapsförmåga"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/Initiativtagare"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <Button
        android:id="@+id/getGame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Get Games"
        android:layout_marginEnd="5dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/Ansvarstagande"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="18dp"
        app:layout_constraintLeft_toLeftOf="@+id/Ansvarstagande"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="5dp" />
</android.support.constraint.ConstraintLayout>
